<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_problema2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.c309660.cn_tema6.Problema2">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="35dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="31dp"
        android:id="@+id/resultTextView"
        android:textStyle="normal|bold"
        android:textSize="18sp"
        android:textColor="@android:color/background_dark" />

    <EditText
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_marginTop="14dp"
        android:id="@+id/editText"
        android:background="@android:color/background_light"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_below="@+id/resultTextView"
        android:layout_toRightOf="@+id/textView4"
        android:layout_toEndOf="@+id/textView4" />

    <TextView
        android:text="s = "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:textStyle="normal|bold"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        android:layout_alignBaseline="@+id/editText"
        android:layout_alignBottom="@+id/editText"
        android:layout_alignLeft="@+id/resultTextView"
        android:layout_alignStart="@+id/resultTextView" />

    <EditText
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_marginTop="14dp"
        android:id="@+id/editText3"
        android:background="@android:color/background_light"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_below="@+id/resultTextView"
        android:layout_toRightOf="@+id/textView4"
        android:layout_toEndOf="@+id/textView4" />

    <EditText
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_marginTop="14dp"
        android:id="@+id/sColoaneEditText"
        android:background="@android:color/background_light"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_below="@+id/resultTextView"
        android:layout_toRightOf="@+id/textView4"
        android:layout_toEndOf="@+id/textView4"
        android:paddingLeft="15dp" />

    <Button
        android:text="Calculeaza"
        android:layout_width="wrap_content"
        android:id="@+id/button2"
        android:layout_marginRight="41dp"
        android:layout_marginEnd="41dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignParentRight="true"
        android:onClick="calculeazaNorma"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="Valorile singulare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:layout_marginTop="33dp"
        android:layout_below="@+id/button2"
        android:layout_alignLeft="@+id/normaTextView"
        android:layout_alignStart="@+id/normaTextView"
        android:textStyle="normal|bold"
        android:textSize="18sp"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:text="Norma ||A - As|| este  :  ?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/normaTextView"
        android:textSize="18sp"
        android:textColor="@android:color/background_dark"
        android:textStyle="normal|bold"
        android:layout_marginTop="47dp"
        android:layout_below="@+id/valoriSingulareTextView"
        android:layout_alignLeft="@+id/matriceaAsView"
        android:layout_alignStart="@+id/matriceaAsView" />

    <TextView
        android:layout_marginTop="17dp"
        android:id="@+id/matriceaAsView"
        android:background="@android:color/background_light"
        android:layout_width="280dp"
        android:layout_height="100dp"
        android:layout_marginRight="13dp"
        android:layout_marginEnd="13dp"
        android:layout_below="@+id/normaTextView"
        android:layout_alignRight="@+id/button2"
        android:layout_alignEnd="@+id/button2" />

    <TextView
        android:layout_width="280dp"
        android:layout_marginTop="16dp"
        android:id="@+id/valoriSingulareTextView"
        android:background="@android:color/background_light"
        android:layout_height="70dp"
        android:layout_below="@+id/textView9"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
